@ECHO off  #@ECHO off,  disables the command prompt from echoing each command as it is executed.
GOTO start
:find_dp0 #find_dp0 label sets the dp0 variable to the directory of the script 
SET dp0=%~dp0
EXIT /b
:start #:start label sets the SETLOCAL command, which starts a new environment scope
SETLOCAL
CALL :find_dp0 #CALL :find_dp0 command sets the dp0 variable to the script's directory

IF EXIST "%dp0%\node.exe" ( ##IF EXIST statement checks if node.exe exists in the script's directory
  SET "_prog=%dp0%\node.exe"
) ELSE (
  SET "_prog=node"
  SET PATHEXT=%PATHEXT:;.JS;=;%
)

endLocal & goto #_undefined_# 2>NUL || title %COMSPEC% & "%_prog%"  "%dp0%\..\js-yaml\bin\js-yaml.js" %*
