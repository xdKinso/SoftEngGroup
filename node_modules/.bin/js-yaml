#!/bin/sh ##!/bin/sh specifies that this script should be executed with the default shell
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')") #The $basedir variable is then set to the directory of the script 

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac
#The dirname command extracts the directory part of the script's path
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
else 
  exec node  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
fi
